<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">
               
    <context:annotation-config/>
	<bean id = "humanHeart" class="com.autowire.Heart">
	<property name="numOfHeart" value="1"/>
	<property name="nameOfAnimal" value="human"/>
	</bean>
	
	<bean id = "octopusHeart" class="com.autowire.Heart">
	<property name="numOfHeart" value="3"/>
	<property name="nameOfAnimal" value="octopus"/>
	</bean>

	<bean id = "human" class="com.autowire.Human" >
	<!-- <property name="heart" ref="heartObject"/> -->
	
	</bean>

</beans>
